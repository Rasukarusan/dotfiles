{
    "diagnostic.enable": false,
    "coc.preferences.jumpCommand": "drop",
    "list.autoResize":false,
    "list.normalMappings": {
        "<C-t>": "action:tabe",
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "languageserver": {
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": ["dockerfile"],
            "args": ["--stdio"]
        }
    }
}
