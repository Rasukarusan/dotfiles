{
  "diagnostic.enable": false,
  "coc.preferences.jumpCommand": "tabe",
  "list.autoResize": false,
  "typescript.autoClosingTags": false,
  "list.normalMappings": {
    "<C-t>": "action:tabe",
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "swift": {
      "command": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
      "filetypes": ["swift"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "tailwindCSS.headwind.runOnSave": false,
  "tailwindCSS.htmlLanguages": ["typescriptreact"],
  "snippets.extends": {
    "typescriptreact": ["typescript"]
  },
  "snippets.userSnippetsDirectory": "~/dotfiles/vim/UltiSnips",
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "css",
    "html",
    "markdown",
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "javascript"
  ],
  "jedi": {
    "enable": true,
    "startupMessage": false,
    "executable.command": "/usr/local/bin/jedi-language-server"
  },
  "snippets.ultisnips.pythonPrompt": false
}
